{
  "name": "@metamask/react-native-acm",
  "version": "0.1.0",
  "description": "Google ACM for react-native",
  "source": "./src/index.tsx",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "lint:changelog": "auto-changelog validate --prettier",
    "lint:constraints": "yarn constraints",
    "lint:dependencies": "depcheck && yarn dedupe",
    "example": "yarn workspace @metamask/react-native-acm-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint:eslint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint": "yarn lint:eslint && yarn lint:changelog && yarn lint:constraints && yarn lint:dependencies",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "prepare": "bob build",
    "build": "yarn prepare",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/react-native-acm.git"
  },
  "author": "ieow <cwlee0@gmail.com> (https://github.com/ieow)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MetaMask/react-native-acm/issues"
  },
  "homepage": "https://github.com/MetaMask/react-native-acm#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@evilmartians/lefthook": "^1.5.0",
    "@lavamoat/allow-scripts": "^3.3.4",
    "@metamask/auto-changelog": "^5.0.2",
    "@react-native-community/cli": "^18.0.0",
    "@react-native/eslint-config": "^0.78.0",
    "@release-it/conventional-changelog": "^9.0.2",
    "@types/jest": "^29.5.5",
    "@types/react": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "commitlint": "^19.6.1",
    "del-cli": "^5.1.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "19.0.0",
    "react-native": "0.78.2",
    "react-native-builder-bob": "^0.38.4",
    "release-it": "^17.10.0",
    "turbo": "^1.10.7",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "create-react-native-library": {
    "type": "legacy-module",
    "languages": "kotlin-swift",
    "version": "0.49.8"
  },
  "packageManager": "yarn@4.9.2",
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false,
      "@evilmartians/lefthook": false
    }
  }
}
